<!-- -*- mode: XML -*- -->

<launch>

  <node pkg="lio" type="lio_processor_node" name="lio_processor" output="log">
  	<param name="rad_diff" type="double" value="1.0" />
  	<param name="infer_start_ori" type="bool" value="false" />
    <remap from="/velodyne_points" to="/velodyne/point_cloud_filtered" />
  </node>
  <node pkg="lio" type="lio_estimator_node" name="lio_estimator" output="screen">
    <param name="config_file" type="string" value="$(find lio)/config/anymal_basement.yaml" />
    <remap from="/imu/data" to="/sensors/imu" />
  </node>

  <!-- <node pkg="lio" type="lio_input_filters_node" name="lio_input_filter" output="screen">
    <remap from="velodyne_points" to="/velodyne/point_cloud_filtered" />
  </node> -->

</launch>